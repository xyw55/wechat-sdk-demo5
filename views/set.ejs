<%- include header%>
    <div class="panel">
        <br>
        <div align="center">
            <form method='post' action="/set" align="center">
                <div class="form-group">
                <h4>区间左边界</h4>
                    <input type="text" class="form-control" name="min" placeholder="区间左边界">
                </div>
                <div class="form-group">
                <h4>区间右边界</h4>
                    <input type="text" class="form-control" name="max" placeholder="区间右边界">
                </div>
                <h4>当前报道区间:(<%= min%>, <%= max%>)</h4>
                <br>
                <button>设置</button>
            </form>
        </div>
    </div>
    <script type="text/javascript" src="http://res.wx.qq.com/open/js/jweixin-1.0.0.js"></script>
    <script type="text/javascript" src="jquery.min.js"></script>
    <script type="text/javascript" src="index.js"></script>
<%- include footer%>
