<%- include header%>
    <div class="panel">
        <div align="center">
            <h4>报道号码</h4>
            <h3><%= art.numid %></h3>
            <h4>学号:<%= art.stuid %></h4>
            <h4>当前报道区间:(<%= min%>, <%= max%>)</h4>
            <% if(art.numid>max){ %>
                <h4>估计等候时间: <%= (art.numid-max)*3%> min</h4>
            <% }else{ %>
                <h4>估计等候时间: 0 min</h4>
            <% } %>
            <br>
            <br>
        </div>
    </div>
    <script type="text/javascript" src="http://res.wx.qq.com/open/js/jweixin-1.0.0.js"></script>
    <script type="text/javascript" src="jquery.min.js"></script>
    <script type="text/javascript" src="index.js"></script>
<%- include footer%>